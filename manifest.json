{
  "manifest_version": 2,

  "name": "twitter-nolarge",
  "description": "This extension utomatically renames .jpg-large files to .jpg",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png"
  },
  "background": {
      "scripts": ["yeah.js"],
      "persistent": true
  },
  "permissions": [
      "downloads"
   ]
}